<#if containerId??>	<#if containerId[0] == '$'>		<#if containerId[1..] == 'document'>			var containerObj = ${containerId[1..]}.documentElement;		<#else>			var containerObj = ${containerId[1..]};		</#if>	<#else>		var containerObj = document.getElementById('${containerId}');	</#if></#if>var draggableOptions${elementId} = {	<#if handleId??>	handle: $('${handleId}'),	</#if>	<#if containerId??>	container: containerObj,	</#if>	<#if snap??>	snap: ${snap},	</#if>	<#if grid??>	grid: ${grid},	</#if>	<#if opacityEffect??>	onStart:function() {		this.element.setOpacity(.5);	},	onComplete:function() {		this.element.setOpacity(1);	},	</#if>	modifiers: {		x: '${modifierX}', 		y: '${modifierY}'	}};$('${elementId}').makeDraggable(draggableOptions${elementId});