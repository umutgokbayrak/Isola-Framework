<#if containerId??>	<#if containerId[0] == '$'>		<#if containerId[1..] == 'document'>			var containerObj = ${containerId[1..]}.documentElement;		<#else>			var containerObj = ${containerId[1..]};		</#if>	<#else>		var containerObj = document.getElementById('${containerId}');	</#if></#if><#if droppables??>var droppables = [<#list droppables as drop>$('${drop}')<#if droppables?last != drop>,</#if></#list>];</#if>var draggableOptions${elementId} = {	<#if handleId??>	handle: $('${handleId}'),	</#if>	<#if containerId??>	container: containerObj,	</#if>	<#if snap??>	snap: ${snap},	</#if>	<#if grid??>	grid: ${grid},	</#if>	<#if onStart??>	onStart: function() {		${onStart}	},	</#if>	<#if onComplete??>	onComplete: function() {		${onComplete}	},	</#if>	<#if onDrag??>	onDrag: function() {		${onDrag}	},	</#if>	<#if droppables??>		droppables: droppables,	</#if>	modifiers: {		x: '${modifierX}', 		y: '${modifierY}'	}};
$('${elementId}').makeDraggable(draggableOptions${elementId});droppables.each(function(drop){ 	drop.addEvents({		<#if onOver??>		'over': function(el){			${onOver}		},		</#if>		<#if onLeave??>		'leave': function(el){			${onLeave}		},		</#if>		<#if onDrop??>		'drop': function(el, obj){			${onDrop}		}		</#if>	});});