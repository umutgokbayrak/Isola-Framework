var ${className} = new Class({	initialize: function(options) {		this.options = options;	},	<#if functions??>	<#list functions as funct>	${funct.name}: function(<#if funct.params??><#if funct.params??><#list funct.params as param>${param.value}<#if funct.params?last != param>,</#if></#list></#if></#if>) {		var params = [];		<#if funct.params??>		<#list funct.params as param>params[params.length] = {type: '${param.name}', value: ${param.value}};</#list>		</#if>		var isola = new Isola();	  	var res = isola.call('${uri}', '${funct.name}', Json.toString(params), this.options);	  	return res;	},	</#list></#if>	destroy: function() {	  	Garbage.empty();	},	uri: '${uri}'});